{
  "command": "lslogins",
  "description": "Show information about users on a Linux system. More information: https://manned.org/lslogins.",
  "useCases": [
    {
      "description": "Display users in the system:",
      "syntax": "lslogins"
    },
    {
      "description": "Display users belonging to a specific group:",
      "syntax": "lslogins --groups={{groups}}"
    },
    {
      "description": "Display user accounts:",
      "syntax": "lslogins --user-accs"
    },
    {
      "description": "Display last logins:",
      "syntax": "lslogins --last"
    },
    {
      "description": "Display system accounts:",
      "syntax": "lslogins --system-accs"
    },
    {
      "description": "Display supplementary groups:",
      "syntax": "lslogins --supp-groups"
    }
  ]
}