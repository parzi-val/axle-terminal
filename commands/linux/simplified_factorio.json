{
  "command": "factorio",
  "description": "Create and start a headless Factorio server. More information: https://wiki.factorio.com/Multiplayer.",
  "useCases": [
    {
      "description": "Create a new save file:",
      "syntax": "{{path/to/factorio}} --create {{path/to/save_file.zip}}"
    },
    {
      "description": "Start a Factorio server:",
      "syntax": "{{path/to/factorio}} --start-server {{path/to/save_file.zip}}"
    }
  ]
}