{
  "command": "pw-metadata",
  "description": "Monitor, set, and delete metadata on PipeWire objects. See also: pipewire, pw-mon, pw-cli. More information: https://docs.pipewire.org/page_man_pw-metadata_1.html.",
  "useCases": [
    {
      "description": "Show metadata in default name:",
      "syntax": "pw-metadata"
    },
    {
      "description": "Show metadata with ID 0 in settings:",
      "syntax": "pw-metadata {{-n|--name}} {{settings}} {{0}}"
    },
    {
      "description": "List all available metadata objects:",
      "syntax": "pw-metadata {{-l|--list}}"
    },
    {
      "description": "Keep running and log the changes to the metadata:",
      "syntax": "pw-metadata {{-m|--monitor}}"
    },
    {
      "description": "Delete all metadata:",
      "syntax": "pw-metadata {{-d|--delete}}"
    },
    {
      "description": "Set log.level to 1 in settings:",
      "syntax": "pw-metadata --name {{settings}} {{0}} {{log.level}} {{1}}"
    },
    {
      "description": "Display help:",
      "syntax": "pw-metadata --help"
    }
  ]
}