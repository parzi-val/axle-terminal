{
  "command": "id3v2",
  "description": "Manage id3v2 tags, converts and lists id3v1. More information: https://manned.org/id3v2.1.",
  "useCases": [
    {
      "description": "List all genres:",
      "syntax": "id3v2 --list-genres"
    },
    {
      "description": "List all tags of specific files:",
      "syntax": "id3v2 --list {{path/to/file1 path/to/file2 ...}}"
    },
    {
      "description": "Delete all id3v2 or id3v1 tags of specific files:",
      "syntax": "id3v2 {{--delete-v2|--delete-v1}} {{path/to/file1 path/to/file2 ...}}"
    },
    {
      "description": "Display help:",
      "syntax": "id3v2 --help"
    },
    {
      "description": "Display version:",
      "syntax": "id3v2 --version"
    }
  ]
}