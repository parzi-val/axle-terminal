{
  "command": "free",
  "description": "Display amount of free and used memory in the system. More information: https://manned.org/free.",
  "useCases": [
    {
      "description": "Display system memory:",
      "syntax": "free"
    },
    {
      "description": "Display memory in Bytes/KB/MB/GB:",
      "syntax": "free -{{b|k|m|g}}"
    },
    {
      "description": "Display memory in human-readable units:",
      "syntax": "free -h"
    },
    {
      "description": "Refresh the output every 2 seconds:",
      "syntax": "free -s {{2}}"
    }
  ]
}