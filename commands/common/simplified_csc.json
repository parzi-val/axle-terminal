{
  "command": "csc",
  "description": "The Microsoft C# Compiler. More information: https://learn.microsoft.com/dotnet/csharp/language-reference/compiler-options/command-line-building-with-csc-exe.",
  "useCases": [
    {
      "description": "Compile one or more C# files to a CIL executable:",
      "syntax": "csc {{path/to/input_file_a.cs}} {{path/to/input_file_b.cs}}"
    },
    {
      "description": "Specify the output filename:",
      "syntax": "csc /out:{{path/to/filename}} {{path/to/input_file.cs}}"
    },
    {
      "description": "Compile into a .dll library instead of an executable:",
      "syntax": "csc /target:library {{path/to/input_file.cs}}"
    },
    {
      "description": "Reference another assembly:",
      "syntax": "csc /reference:{{path/to/library.dll}} {{path/to/input_file.cs}}"
    },
    {
      "description": "Embed a resource:",
      "syntax": "csc /resource:{{path/to/resource_file}} {{path/to/input_file.cs}}"
    },
    {
      "description": "Automatically generate XML documentation:",
      "syntax": "csc /doc:{{path/to/output.xml}} {{path/to/input_file.cs}}"
    },
    {
      "description": "Specify an icon:",
      "syntax": "csc /win32icon:{{path/to/icon.ico}} {{path/to/input_file.cs}}"
    },
    {
      "description": "Strongly-name the resulting assembly with a keyfile:",
      "syntax": "csc /keyfile:{{path/to/keyfile}} {{path/to/input_file.cs}}"
    }
  ]
}