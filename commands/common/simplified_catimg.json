{
  "command": "catimg",
  "description": "Image printing in the terminal. See also: pixterm, chafa. More information: https://github.com/posva/catimg.",
  "useCases": [
    {
      "description": "Print a JPEG, PNG, or GIF to the terminal:",
      "syntax": "catimg {{path/to/file}}"
    },
    {
      "description": "Double the [r]esolution of an image:",
      "syntax": "catimg -r 2 {{path/to/file}}"
    },
    {
      "description": "Disable 24-bit color for better [t]erminal support:",
      "syntax": "catimg -t {{path/to/file}}"
    },
    {
      "description": "Specify a custom [w]idth or [H]eight:",
      "syntax": "catimg {{-w|-H}} {{40}} {{path/to/file}}"
    }
  ]
}