{
  "command": "audtool",
  "description": "Control Audacious using commands. See also: audacious. More information: https://manned.org/audtool.",
  "useCases": [
    {
      "description": "Play/pause audio playback:",
      "syntax": "audtool playback-playpause"
    },
    {
      "description": "Print artist, album, and song name of currently playing song:",
      "syntax": "audtool current-song"
    },
    {
      "description": "Set volume of audio playback:",
      "syntax": "audtool set-volume {{100}}"
    },
    {
      "description": "Skip to the next song:",
      "syntax": "audtool playlist-advance"
    },
    {
      "description": "Print the bitrate of the current song in kilobits:",
      "syntax": "audtool current-song-bitrate-kbps"
    },
    {
      "description": "Open Audacious in full-screen if hidden:",
      "syntax": "audtool mainwin-show"
    },
    {
      "description": "Display help:",
      "syntax": "audtool help"
    },
    {
      "description": "Display settings:",
      "syntax": "audtool preferences-show"
    }
  ]
}