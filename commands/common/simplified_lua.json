{
  "command": "lua",
  "description": "A powerful, light-weight embeddable programming language. More information: https://www.lua.org.",
  "useCases": [
    {
      "description": "Start an interactive Lua shell:",
      "syntax": "lua"
    },
    {
      "description": "Execute a Lua script:",
      "syntax": "lua {{path/to/script.lua}} {{--optional-argument}}"
    },
    {
      "description": "Execute a Lua expression:",
      "syntax": "lua -e '{{print(&quot;Hello World&quot;)}}'"
    }
  ]
}