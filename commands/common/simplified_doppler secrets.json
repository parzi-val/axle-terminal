{
  "command": "doppler secrets",
  "description": "Manage your Doppler project's secrets. More information: https://docs.doppler.com/docs/accessing-secrets.",
  "useCases": [
    {
      "description": "Get all secrets:",
      "syntax": "doppler secrets"
    },
    {
      "description": "Get value(s) of one or more secrets:",
      "syntax": "doppler secrets get {{secrets}}"
    },
    {
      "description": "Upload a secrets file:",
      "syntax": "doppler secrets upload {{path/to/file.env}}"
    },
    {
      "description": "Delete value(s) of one or more secrets:",
      "syntax": "doppler secrets delete {{secrets}}"
    },
    {
      "description": "Download secrets as .env:",
      "syntax": "doppler secrets download --format=env --no-file &gt; {{path/to/.env}}"
    }
  ]
}