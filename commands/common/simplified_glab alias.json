{
  "command": "glab alias",
  "description": "Manage GitLab CLI command aliases. More information: https://glab.readthedocs.io/en/latest/alias.",
  "useCases": [
    {
      "description": "Display the subcommand help:",
      "syntax": "glab alias"
    },
    {
      "description": "List all the aliases glab is configured to use:",
      "syntax": "glab alias list"
    },
    {
      "description": "Create a glab subcommand alias:",
      "syntax": "glab alias set {{mrv}} '{{mr view}}'"
    },
    {
      "description": "Set a shell command as a glab subcommand:",
      "syntax": "glab alias set --shell {{alias_name}} {{command}}"
    },
    {
      "description": "Delete a command shortcut:",
      "syntax": "glab alias delete {{alias_name}}"
    }
  ]
}