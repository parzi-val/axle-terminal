{
  "command": "az redis",
  "description": "Manage Redis caches. Part of azure-cli (also known as az). More information: https://learn.microsoft.com/cli/azure/redis.",
  "useCases": [
    {
      "description": "Create a new Redis cache instance:",
      "syntax": "az redis create --location {{location}} --name {{name}} --resource-group {{resource_group}} --sku {{Basic|Premium|Standard}} --vm-size {{c0|c1|c2|c3|c4|c5|c6|p1|p2|p3|p4|p5}}"
    },
    {
      "description": "Update a Redis cache:",
      "syntax": "az redis update --name {{name}} --resource-group {{resource_group}} --sku {{Basic|Premium|Standard}} --vm-size {{c0|c1|c2|c3|c4|c5|c6|p1|p2|p3|p4|p5}}"
    },
    {
      "description": "Export data stored in a Redis cache:",
      "syntax": "az redis export --container {{container}} --file-format {{file-format}} --name {{name}} --prefix {{prefix}} --resource-group {{resource_group}}"
    },
    {
      "description": "Delete a Redis cache:",
      "syntax": "az redis delete --name {{name}} --resource-group {{resource_group}} --yes"
    }
  ]
}