{
  "command": "nth",
  "description": "Name That Hash - Instantly name the type of any hash. More information: https://github.com/hashpals/name-that-hash.",
  "useCases": [
    {
      "description": "Name a hash:",
      "syntax": "nth -t {{5f4dcc3b5aa765d61d8327deb882cf99}}"
    },
    {
      "description": "Name hashes in a file:",
      "syntax": "nth -f {{path/to/hashes}}"
    },
    {
      "description": "Print in JSON format:",
      "syntax": "nth -t {{5f4dcc3b5aa765d61d8327deb882cf99}} -g"
    },
    {
      "description": "Decode hash in Base64 before naming it:",
      "syntax": "nth -t {{NWY0ZGNjM2I1YWE3NjVkNjFkODMyN2RlYjg4MmNmOTkK}} -b64"
    }
  ]
}