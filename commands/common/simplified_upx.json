{
  "command": "upx",
  "description": "Compress or decompress executables. More information: https://upx.github.io.",
  "useCases": [
    {
      "description": "Compress executable:",
      "syntax": "upx {{path/to/file}}"
    },
    {
      "description": "Decompress executable:",
      "syntax": "upx -d {{path/to/file}}"
    },
    {
      "description": "Detailed help:",
      "syntax": "upx --help"
    }
  ]
}