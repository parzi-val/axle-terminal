{
  "command": "docker logs",
  "description": "Print container logs. More information: https://docs.docker.com/reference/cli/docker/container/logs/.",
  "useCases": [
    {
      "description": "Print logs from a container:",
      "syntax": "docker logs {{container_name}}"
    },
    {
      "description": "Print logs and follow them:",
      "syntax": "docker logs -f {{container_name}}"
    },
    {
      "description": "Print last 5 lines:",
      "syntax": "docker logs {{container_name}} --tail {{5}}"
    },
    {
      "description": "Print logs and append them with timestamps:",
      "syntax": "docker logs -t {{container_name}}"
    },
    {
      "description": "Print logs from a certain point in time of container execution (i.e. 23m, 10s, 2013-01-02T13:23:37):",
      "syntax": "docker logs {{container_name}} --until {{time}}"
    }
  ]
}