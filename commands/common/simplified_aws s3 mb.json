{
  "command": "aws s3 mb",
  "description": "Create S3 buckets. More information: https://awscli.amazonaws.com/v2/documentation/api/latest/reference/s3/mb.html.",
  "useCases": [
    {
      "description": "Create an S3 bucket:",
      "syntax": "aws s3 mb s3://{{bucket_name}}"
    },
    {
      "description": "Create an S3 bucket in a specific region:",
      "syntax": "aws s3 mb s3://{{bucket_name}} --region {{region}}"
    },
    {
      "description": "Display help:",
      "syntax": "aws s3 mb help"
    }
  ]
}