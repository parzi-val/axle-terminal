{
  "command": "ansible-galaxy",
  "description": "Perform various Ansible Role and Collection related operations. More information: https://docs.ansible.com/ansible/latest/cli/ansible-galaxy.html.",
  "useCases": [
    {
      "description": "List installed roles or collections:",
      "syntax": "ansible-galaxy {{role|collection}} list"
    },
    {
      "description": "Search for a role with various levels of verbosely (-v should be specified at the end):",
      "syntax": "ansible-galaxy role search {{keyword}} -v{{vvvvv}}"
    },
    {
      "description": "Install or remove role(s):",
      "syntax": "ansible-galaxy role {{install|remove}} {{role_name1 role_name2 ...}}"
    },
    {
      "description": "Create a new role:",
      "syntax": "ansible-galaxy role init {{role_name}}"
    },
    {
      "description": "Get information about a role:",
      "syntax": "ansible-galaxy role info {{role_name}}"
    },
    {
      "description": "Install or remove collection(s):",
      "syntax": "ansible-galaxy collection {{install|remove}} {{collection_name1 collection_name2 ...}}"
    },
    {
      "description": "Display help about roles or collections:",
      "syntax": "ansible-galaxy {{role|collection}} {{-h|--help}}"
    }
  ]
}