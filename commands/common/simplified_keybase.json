{
  "command": "keybase",
  "description": "Key directory that maps social media identities to encryption keys in a publicly auditable manner. More information: https://keybase.io/docs/command_line.",
  "useCases": [
    {
      "description": "Follow another user:",
      "syntax": "keybase follow {{username}}"
    },
    {
      "description": "Add a new proof:",
      "syntax": "keybase prove {{service}} {{service_username}}"
    },
    {
      "description": "Sign a file:",
      "syntax": "keybase sign --infile {{input_file}} --outfile {{output_file}}"
    },
    {
      "description": "Verify a signed file:",
      "syntax": "keybase verify --infile {{input_file}} --outfile {{output_file}}"
    },
    {
      "description": "Encrypt a file:",
      "syntax": "keybase encrypt --infile {{input_file}} --outfile {{output_file}} {{receiver}}"
    },
    {
      "description": "Decrypt a file:",
      "syntax": "keybase decrypt --infile {{input_file}} --outfile {{output_file}}"
    },
    {
      "description": "Revoke current device, log out, and delete local data:",
      "syntax": "keybase deprovision"
    }
  ]
}