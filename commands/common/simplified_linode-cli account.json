{
  "command": "linode-cli account",
  "description": "Manage Linode accounts. See also: linode-cli. More information: https://techdocs.akamai.com/cloud-computing/docs/cli-commands-for-account-management.",
  "useCases": [
    {
      "description": "View account:",
      "syntax": "linode-cli account view"
    },
    {
      "description": "View account settings:",
      "syntax": "linode-cli account settings"
    },
    {
      "description": "Make a payment:",
      "syntax": "linode-cli account payment-create --cvv {{cvv}} --usd {{amount_in_dollars}}"
    },
    {
      "description": "View account notifications:",
      "syntax": "linode-cli account notifications-list"
    }
  ]
}