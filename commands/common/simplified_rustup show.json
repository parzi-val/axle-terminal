{
  "command": "rustup show",
  "description": "Show installed toolchains, targets and the version of rustc. More information: https://rust-lang.github.io/rustup.",
  "useCases": [
    {
      "description": "Show all information:",
      "syntax": "rustup show"
    },
    {
      "description": "Show the active toolchain:",
      "syntax": "rustup show active-toolchain"
    },
    {
      "description": "Show the rustup data directory:",
      "syntax": "rustup show home"
    }
  ]
}