{
  "command": "newman",
  "description": "Collection runner for Postman. More information: https://github.com/postmanlabs/newman.",
  "useCases": [
    {
      "description": "Run a collection (from a file):",
      "syntax": "newman run {{path/to/collection.json}}"
    },
    {
      "description": "Run a collection (from a URL):",
      "syntax": "newman run {{https://www.getpostman.com/collections/631643-f695cab7-6878-eb55-7943-ad88e1ccfd65-JsLv}}"
    }
  ]
}