{
  "command": "git revert",
  "description": "Create new commits which reverse the effect of earlier ones. More information: https://git-scm.com/docs/git-revert.",
  "useCases": [
    {
      "description": "Revert the most recent commit:",
      "syntax": "git revert {{HEAD}}"
    },
    {
      "description": "Revert the 5th last commit:",
      "syntax": "git revert HEAD~{{4}}"
    },
    {
      "description": "Revert a specific commit:",
      "syntax": "git revert {{0c01a9}}"
    },
    {
      "description": "Revert multiple commits:",
      "syntax": "git revert {{branch_name~5..branch_name~2}}"
    },
    {
      "description": "Don't create new commits, just change the working tree:",
      "syntax": "git revert {{-n|--no-commit}} {{0c01a9..9a1743}}"
    }
  ]
}