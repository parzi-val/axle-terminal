{
  "command": "mcs",
  "description": "Mono C# Compiler. More information: https://manned.org/mcs.1.",
  "useCases": [
    {
      "description": "Compile the specified files:",
      "syntax": "mcs {{path/to/input_file1.cs path/to/input_file2.cs ...}}"
    },
    {
      "description": "Specify the output program name:",
      "syntax": "mcs -out:{{path/to/file.exe}} {{path/to/input_file1.cs path/to/input_file2.cs ...}}"
    },
    {
      "description": "Specify the output program type:",
      "syntax": "mcs -target:{{exe|winexe|library|module}} {{path/to/input_file1.cs path/to/input_file2.cs ...}}"
    }
  ]
}